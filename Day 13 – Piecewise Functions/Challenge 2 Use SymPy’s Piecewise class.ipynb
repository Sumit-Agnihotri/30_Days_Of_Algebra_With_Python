   "source": [
    "from sympy import symbols, Piecewise\n",
    "\n",
    "x = symbols('x')\n",
    "f = Piecewise((x**2, x < 0), (x + 2, x >= 0))\n",
    "\n",
    "print(\"f(x):\", f)\n",
    "print(\"f(-3):\", f.subs(x, -3))\n",
    "print(\"f(2):\", f.subs(x, 2))"
   ]
  }
